extends layouts/_layout.pug

block variables
	- var activePage = 'basic-table'
	- var activeGroup = 'tables'

block title
	title #{title} | Blog about nothing and everything

block content
	.app-title
		div
			h1
				i.fa.fa-th-list
				|  Users list
			p Users registered on the website

	.row
